<?xml version="1.0" encoding="UTF-8"?>
<tns:projectTemplate xmlns:tns="http://fdt.powerflasher.com/ProjectTemplate"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://fdt.powerflasher.com/ProjectTemplate ../../projectTemplate.xsd">
	<name>Basilicom BasCore for VZ</name>
	<description>In addition to the libraries for Flex 4 development, this template contains special libraries for unit testing with FlexUnit 4.</description>
	<projectType>Flex 4</projectType>
	<folders>
		<sourceFolder>src</sourceFolder>
		<sourceFolder>src-test</sourceFolder>
		<outputFolder>bin-debug</outputFolder>
	</folders>
	<libraries>
		<swc>
			<path>libs/flexunit-4.0.0.swc</path>
			<asDoc>http://docs.flexunit.org/asdocs/index.html</asDoc>
			<source></source>
		</swc>
		<swc>
			<path>libs/flexunit-uilistener-4.0.0.swc</path>
			<asDoc>http://docs.flexunit.org/asdocs/index.html</asDoc>
			<source></source>
		</swc>
		<swc>
			<path>libs/BasCore_FDT>swc</path>
		</swc>
	</libraries>
</tns:projectTemplate>
